{
  "expo": {
    "name": "dojo",
    "slug": "ReactNativeUI",
    "scheme": ["dojo", "fb3497198713853522" ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yanivamon.dojo",
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yanivamon.dojo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "811ff38d-f7f3-4a22-8698-637d5331382c"
      }
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication"
    ]
  }
}